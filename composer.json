{
    "name": "tuandm/laravue-core",
    "description": "A Laravel package to provide a beatiful datashboard built by VueJS and Element-UI",
    "type": "library",
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0",
        "laravel/passport": "^10.3",
        "laravel/socialite": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "Tuandm\\Laravue\\": "src",
            "Tuandm\\Laravue\\Database\\Seeds\\": "src/database/seeds"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tuandm\\Laravue\\ServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Tuan Duong",
            "email": "bacduong@gmail.com"
        }
    ],
    "minimum-stability": "dev"
}
